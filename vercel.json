{
  "version": 2,
  "builds": [{
    "src": "app.py",
    "use": "@vercel/python",
    "config": {
      "pythonVersion": "3.10",
      "maxLambdaSize": "50mb",
      "installCommand": "pip install --no-cache-dir -r requirements.txt"
    }
  }],
  "routes": [{
    "src": "/(.*)",
    "dest": "app.py"
  }]
}